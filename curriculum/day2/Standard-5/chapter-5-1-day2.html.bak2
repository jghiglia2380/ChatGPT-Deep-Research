    <script>
        // Station navigation and tracking
        let currentStation = 0;
        const stations = document.querySelectorAll('.learning-station');
        const stationIndicators = document.querySelectorAll('.station-indicator');
        
        // Initialize interactive elements
        document.addEventListener('DOMContentLoaded', () => {
            // Set up compounding calculator
            initializeCompoundingCalculator();
            
            // Set up investment portfolio simulator
            initializePortfolioSimulator();
            
            // Set up opportunity cost calculator
            initializeOpportunityCostCalculator();
            
            // Add smooth transitions between stations
            stations.forEach((station, index) => {
                station.style.opacity = '0';
                station.style.transform = 'translateY(20px)';
                
                // Use setTimeout to create a staggered animation effect
                setTimeout(() => {
                    station.style.opacity = '1';
                    station.style.transform = 'translateY(0)';
                    station.style.transition = 'opacity 0.5s ease, transform 0.5s ease';
                }, 100 * (index + 1));
            });
            
            updateStationIndicators();
        });
        // Compounding Calculator
        function initializeCompoundingCalculator() {
            const initialAmount = document.getElementById('initial-amount');
            const interestRate = document.getElementById('interest-rate');
            const timePeriod = document.getElementById('time-period');
            const monthlyContribution = document.getElementById('monthly-contribution');
            const calculateBtn = document.getElementById('calculate-btn');
            
            // Update displayed values for sliders
            interestRate.addEventListener('input', () => {
                document.getElementById('interest-rate-value').textContent = interestRate.value + '%';
            });
            
            timePeriod.addEventListener('input', () => {
                document.getElementById('time-period-value').textContent = timePeriod.value + ' years';
            });
            
            // Calculate button click handler
            calculateBtn.addEventListener('click', () => {
                const principal = parseFloat(initialAmount.value);
                const rate = parseFloat(interestRate.value) / 100;
                const years = parseInt(timePeriod.value);
                const monthly = parseFloat(monthlyContribution.value);
                
                // Calculate future value
                let futureValue = principal * Math.pow(1 + rate, years);
                
                // Add monthly contributions
                if (monthly > 0) {
                    // Future value of a regular payment series
                    const monthlyFuture = monthly * ((Math.pow(1 + (rate/12), years * 12) - 1) / (rate/12));
                    futureValue += monthlyFuture;
                }
                
                // Calculate total contributions
                const totalContributions = principal + (monthly * 12 * years);
                
                // Calculate interest earned
                const interestEarned = futureValue - totalContributions;
                
                // Update results
                document.getElementById('final-balance').textContent = '$' + futureValue.toFixed(2).replace(/\B(?=(\d{3})+(?\!\d))/g, ",");
                document.getElementById('total-contributions').textContent = '$' + totalContributions.toFixed(2).replace(/\B(?=(\d{3})+(?\!\d))/g, ",");
                document.getElementById('interest-earned').textContent = '$' + interestEarned.toFixed(2).replace(/\B(?=(\d{3})+(?\!\d))/g, ",");
                
                // Create chart data
                createGrowthChart(principal, rate, years, monthly);
                
                showToast('Calculation complete\!');
            });
        }
        
        // Create growth chart for compound interest calculator
        function createGrowthChart(principal, rate, years, monthly) {
            const ctx = document.getElementById('growth-chart').getContext('2d');
            let labels = [];
            let principalData = [];
            let contributionsData = [];
            let interestData = [];
            
            // Calculate data points for each year
            for (let year = 0; year <= years; year++) {
                labels.push('Year ' + year);
                
                let totalContributions = principal + (monthly * 12 * year);
                
                // Year 0 has no interest
                if (year === 0) {
                    principalData.push(principal);
                    contributionsData.push(0);
                    interestData.push(0);
                    continue;
                }
                
                // Calculate future value at this year
                let futureValue = principal * Math.pow(1 + rate, year);
                
                // Add monthly contributions
                if (monthly > 0) {
                    // Future value of a regular payment series
                    const monthlyFuture = monthly * ((Math.pow(1 + (rate/12), year * 12) - 1) / (rate/12));
                    futureValue += monthlyFuture;
                }
                
                // Calculate interest earned
                const interestEarned = futureValue - totalContributions;
                
                principalData.push(principal);
                contributionsData.push(totalContributions - principal);
                interestData.push(interestEarned);
            }
            
            // Destroy existing chart if it exists
            if (window.growthChart) {
                window.growthChart.destroy();
            }
            
            // Create new chart
            window.growthChart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: labels,
                    datasets: [
                        {
                            label: 'Initial Investment',
                            data: principalData,
                            backgroundColor: 'rgba(79, 70, 229, 0.7)',
                            borderColor: 'rgba(79, 70, 229, 1)',
                            borderWidth: 1
                        },
                        {
                            label: 'Additional Contributions',
                            data: contributionsData,
                            backgroundColor: 'rgba(99, 102, 241, 0.5)',
                            borderColor: 'rgba(99, 102, 241, 1)',
                            borderWidth: 1
                        },
                        {
                            label: 'Interest Earned',
                            data: interestData,
                            backgroundColor: 'rgba(52, 211, 153, 0.7)',
                            borderColor: 'rgba(52, 211, 153, 1)',
                            borderWidth: 1
                        }
                    ]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: true,
                    scales: {
                        x: {
                            stacked: true
                        },
                        y: {
                            stacked: true,
                            ticks: {
                                callback: function(value) {
                                    return '$' + value.toLocaleString();
                                }
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            position: 'top',
                        },
                        tooltip: {
                            callbacks: {
                                label: function(context) {
                                    return context.dataset.label + ': $' + context.raw.toLocaleString();
                                }
                            }
                        }
                    }
                }
            });
        }
        // Investment Portfolio Simulator
        function initializePortfolioSimulator() {
            // College savings portfolio
            const collegeInputs = ['college-cash', 'college-bonds', 'college-stocks', 'college-alt'];
            collegeInputs.forEach(id => {
                document.getElementById(id).addEventListener('input', () => updatePortfolio('college'));
            });
            
            // Retirement savings portfolio
            const retirementInputs = ['retirement-cash', 'retirement-bonds', 'retirement-stocks', 'retirement-alt'];
            retirementInputs.forEach(id => {
                document.getElementById(id).addEventListener('input', () => updatePortfolio('retirement'));
            });
            
            // Home down payment portfolio
            const homeInputs = ['home-cash', 'home-bonds', 'home-stocks', 'home-alt'];
            homeInputs.forEach(id => {
                document.getElementById(id).addEventListener('input', () => updatePortfolio('home'));
            });
            
            // Initialize portfolios
            updatePortfolio('college');
            updatePortfolio('retirement');
            updatePortfolio('home');
        }
        // Basic functions
        function saveStation(stationNumber) {
            document.getElementById(`station${stationNumber}-indicator`).classList.add('active');
            updateProgress();
            showToast(`Station ${stationNumber} completed\!`);
        }
        
        function savePortfolio() {
            document.getElementById('portfolio-indicator').classList.add('active');
            updateProgress();
            showToast('Portfolio saved\!');
        }
        
        function showToast(message) {
            const toast = document.getElementById('toast');
            document.getElementById('toast-message').textContent = message;
            toast.classList.add('show');
            setTimeout(() => {toast.classList.remove('show');}, 3000);
        }
        
        function updateProgress() {
            const completed = document.querySelectorAll('.station-indicator.active').length;
            const total = document.querySelectorAll('.station-indicator').length;
            const percentage = Math.round((completed / total) * 100);
            document.querySelector('.from-indigo-600.to-purple-600.h-2\\.5').style.width = `${percentage}%`;
            document.querySelector('.font-medium').textContent = `${completed}/${total} stations`;
        }
        
        function updateStationIndicators() {
            updateProgress();
        }
    </script>
